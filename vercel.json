{
  "functions": {
    "pages/api/chat.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_SERVICE_ACCOUNT_EMAIL": "@google_service_account_email", 
    "GOOGLE_PRIVATE_KEY": "@google_private_key"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}